    <tr><td>web/bin</td>           <td><i>Web</i> executables</td></tr>
    <tr><td>web/docs</td>          <td><i>Web</i> documentation, html pages</td></tr>
    <tr><td>web/rgb</td>           <td>RGB color look-up-tables</td></tr>
    <tr><td>web/jweb</td>          <td>JWeb directory</td></tr>  
