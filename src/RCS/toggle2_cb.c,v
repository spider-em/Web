#include <Xm/ToggleBG.h>

 /********************  toggle2_cb   ****************************/

 void  toggle2_cb(Widget iw_temp, int *data, caddr_t call_data)
 {
   *data = XmToggleButtonGadgetGetState(iw_temp); 
 }

