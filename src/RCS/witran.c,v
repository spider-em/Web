@Initial revision
@
text
@d1 1
a1 1
/*$Header$*/
d18 1
d21 1
a21 1
const float pi = 3.1415927;
d23 3
a25 3
rthe = the / 180 * pi;
rphi = phi / 180 * pi;
rgam = gam / 180 * pi;
d33 3
d38 1
a38 1
    xs[i] =  (x[i] * cgam - y[i] * sgam) * cthe * cphi
d40 2
a41 1
    ys[i] = -(x[i] * cgam - y[i] * sgam) * cthe * sphi
d44 1
a47 22
/*
        DIMENSION X(N),Y(N),XS(N),YS(N)
	INCLUDE 'COMMON1:WIRCT.INC'

        PI=3.1415927
        RGAM=GAM/180.*PI
        RTHE=THE/180.*PI
        RPHI=PHI/180.*PI
        CGAM=COS(RGAM)
        SGAM=SIN(RGAM)
        CPHI=COS(RPHI)
        SPHI=SIN(RPHI)
        CTHE=COS(RTHE)
        DO 1 I=1,N
        XS(I)=((X(I)-XU0)*CGAM-(Y(I)-YU0)*SGAM)*CTHE*CPHI
     $     +((X(I)-XU0)*SGAM+(Y(I)-YU0)*CGAM)*SPHI+XS0
        YS(I)=-((X(I)-XU0)*CGAM-(Y(I)-YU0)*SGAM)*CTHE*SPHI
     $     +((X(I)-XU0)*SGAM+(Y(I)-YU0)*CGAM)*CPHI+YS0
1       CONTINUE
        RETURN
        END
