#include "files.h"

 int closefile(FILEDATA *fileptr)

 {
 if (fclose(fileptr -> fp)) 
     {
     printf(" *** Error closing file. \n");
     return FALSE;
     }
 /***** ? fileptr->fp =  NULL;   */

 /* free the line buffer in the structure */
 if (fileptr->buf) free(fileptr->buf);

 /* free the whole structure */
 free (fileptr);
 
 return TRUE;
 }
