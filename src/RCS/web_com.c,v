 /*  function prototypes */
 extern Widget  wid_pulldown (Widget, char *, int);
 extern Widget  wid_pushg    (Widget, Widget, char *, void (*cb)
                              (Widget,caddr_t,caddr_t), char *,int,int);


 extern void    imoptmen   (Widget, caddr_t, caddr_t);
 extern void    colmen     (Widget, caddr_t, caddr_t);
 extern void    curmen     (Widget, caddr_t, caddr_t);
 extern void    lutmen     (Widget, caddr_t, caddr_t);
 extern void    contrastmen(Widget, caddr_t, caddr_t);

 extern void    web_info  (Widget, caddr_t, caddr_t);
 
 extern void    imagemen  (Widget, caddr_t, caddr_t);
 extern void    itex      (Widget, caddr_t, caddr_t);
 extern void    webclear  (Widget, caddr_t, caddr_t);
 extern void    winsiz    (Widget, caddr_t, caddr_t);
 extern void    pixelmen  (Widget, caddr_t, caddr_t);
 extern void    rubberline(Widget, caddr_t, caddr_t);
 extern void    blackout  (Widget, caddr_t, caddr_t);
 extern void    bar       (Widget, caddr_t, caddr_t);
